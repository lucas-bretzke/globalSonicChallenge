generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Device {
  id         Int      @id @default(autoincrement())
  name       String
  mac        String   @unique
  status     Status   @default(ATIVO)
  created_at DateTime @default(now())
}

enum Status {
  ATIVO
  INATIVO
}
